{
    "name": "okeyiyke/fcm-notifications",
    "description": "FCM Notifications Driver for Laravel v12",
    "license": "MIT",
    "authors": [
        {
            "name": "John Okechukwu",
            "email": "okeyiyke@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/support": "^^10.0|^11.0|^12.0",
        "tomatophp/laravel-firebase": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Fcm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Fcm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/IykeOkey/fcm-notifications"
    }
]

}
